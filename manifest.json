{
	"manifest_version": 2,

	"name": "Tic Tac Toe",
	"description": "Allows you to play multiplayer Tic Tac Toe.",
	"version": "1.0",

	"browser_action": {
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab",
		"storage"
	],
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}